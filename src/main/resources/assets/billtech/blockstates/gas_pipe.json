{
  "multipart": [
    { "apply": { "model": "billtech:block/gas_pipe_center" } },
    { "when": { "north": "pipe" }, "apply": { "model": "billtech:block/gas_pipe_arm" } },
    { "when": { "south": "pipe" }, "apply": { "model": "billtech:block/gas_pipe_arm", "y": 180 } },
    { "when": { "east": "pipe" }, "apply": { "model": "billtech:block/gas_pipe_arm", "y": 90 } },
    { "when": { "west": "pipe" }, "apply": { "model": "billtech:block/gas_pipe_arm", "y": 270 } },
    { "when": { "up": "pipe" }, "apply": { "model": "billtech:block/gas_pipe_arm", "x": -90 } },
    { "when": { "down": "pipe" }, "apply": { "model": "billtech:block/gas_pipe_arm", "x": 90 } },
    { "when": { "north": "endpoint" }, "apply": { "model": "billtech:block/gas_pipe_end" } },
    { "when": { "south": "endpoint" }, "apply": { "model": "billtech:block/gas_pipe_end", "y": 180 } },
    { "when": { "east": "endpoint" }, "apply": { "model": "billtech:block/gas_pipe_end", "y": 90 } },
    { "when": { "west": "endpoint" }, "apply": { "model": "billtech:block/gas_pipe_end", "y": 270 } },
    { "when": { "up": "endpoint" }, "apply": { "model": "billtech:block/gas_pipe_end", "x": -90 } },
    { "when": { "down": "endpoint" }, "apply": { "model": "billtech:block/gas_pipe_end", "x": 90 } }
  ]
}
